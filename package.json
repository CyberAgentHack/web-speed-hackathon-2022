  "scripts": {
    "client": "yarn workspace @wsh/client",
    "server": "yarn workspace @wsh/server",
    "prod:client": "yarn build:client && yarn serve:client",
    "prod:server": "yarn build:server && yarn serve:server",
    "build": "yarn clean && yarn build:client && yarn build:server",
    "build:client": "yarn client build",
    "build:server": "yarn server build",
    "serve": "yarn serve:client && yarn serve:server",
    "serve:client": "yarn client start",
    "serve:server": "yarn server serve",
    "clean": "rimraf src/client/dist src/server/dist",
    "gen": "scaffdog generate",
    "format": "deno fmt",
    "lint": "deno lint --unstable",
    "init:seeds": "ts-node-transpile-only scripts/init.js"
  },
